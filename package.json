    "npm-check": "^5.9.0",
    "prettier": "^1.16.4",
    "react-svg-loader": "^3.0.1",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
