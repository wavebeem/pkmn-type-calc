  "scripts": {
    "upgrade": "npm-check -u",
    "start": "webpack-dev-server --open",
    "build:ico": "bash bin/make-ico",
    "build:pkmn": "ruby bin/make-pkmn",
    "build:bundle": "webpack -p",
